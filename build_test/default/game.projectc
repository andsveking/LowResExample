[project]
title = LowResExample
version = 0.1

[bootstrap]
main_collection = /res-selection/res-dialog.collectionc
render = /render/lowres.renderc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 300
height = 300

[physics]
scale = 0.02
gravity_y = -100

[script]
shared_state = 1

[ios]
bundle_identifier = com.defold.lowresexample

[android]
package = com.defold.lowresexample

[osx]
bundle_identifier = com.defold.lowresexample

[graphics]
default_texture_min_filter = nearest
default_texture_mag_filter = nearest

